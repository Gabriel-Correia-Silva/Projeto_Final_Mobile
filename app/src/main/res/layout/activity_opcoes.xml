<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".utils.opcoes">
    <Button
        android:id="@+id/botao_retornar_main_screen"
        android:layout_width="60sp"
        android:layout_height="60sp"
        android:backgroundTint="#00FFFFFF"
        android:drawableRight="@drawable/caret_right_4"
        android:gravity="center"
        android:padding="21sp"
        app:layout_constraintStart_toStartOf="parent" />




    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/sugestoes"
        android:layout_width="350sp"
        android:layout_height="50sp"
        android:layout_marginTop="60sp"
        android:background="@drawable/rounded_to_button"
        android:text="Suas Sugestões"
        android:textColor="#FFFFFF"
        android:textColorHint="@color/cinza"
        android:layout_gravity="center"
        android:gravity="center"
         />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/notificacoes"
        android:layout_width="350sp"
        android:layout_height="50sp"
        android:layout_marginTop="60sp"
        android:background="@drawable/rounded_to_button"
        android:text="Notificações"
        android:textColor="#FFFFFF"
        android:textColorHint="@color/cinza"
        android:layout_gravity="center"
        android:gravity="center"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/deslogar"
        android:layout_width="350sp"
        android:layout_height="50sp"
        android:layout_marginTop="60sp"
        android:background="@drawable/rounded_to_button"
        android:text="Deslogar"
        android:textColor="#FFFFFF"
        android:textColorHint="@color/cinza"
        android:layout_gravity="center"
        android:gravity="center"
        />
</LinearLayout>